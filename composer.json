{
    "name": "ftrotter/durcc",
    "description": "DURC is reverse CRUD",
    "type": "library",
    "license": "MIT",
     "keywords": [
		"laravel",
		"crud",
		"orm",
		"mysql",
		"mariadb"
    ],
    "homepage": "https://github.com/ftrotter/DURCC",
    "authors": [
        {
            "name": "Fred Trotter",
            "email": "fred.trotter@gmail.com",
            "homepage": "https://www.fredtrotter.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {

        "php" : "^8.1",
        "erusev/parsedown": "^1.7",
        "zordius/lightncandy": "dev-master"

    },
    "psr-4": {
            "App\\": "app/",
            "ftrotter\\DURCC\\": "packages/CareSet/durc/src"
    },
    "autoload": {
	"psr-4": {
		"ftrotter\\DURCC\\": "src/"
	}
    },
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public"
    ],
	"extra": {
		"laravel": {
			"providers": [
				"ftrotter\\DURCC\\DURCServiceProvider"
			]
		}
	},
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
